.projectList{
  background-color: #FEFEFE;
}
.projectList-img>image{
    width: 100%;
    height: 278rpx;
}
.projectList-list{
  margin: 0 25rpx; 
  margin-bottom: 73rpx;
}
.projectList-item-1{
  display: flex;
  justify-content: space-between;
}
.projectList-item-1,.projectList-item-2,.projectList-item-3{
  padding: 10rpx 0;
}
.item-1-left{
  font-size: 35rpx;
}
.item-1-right{
  font-size: 20rpx;
  color: #A7A7A7;
}
.projectList-item-2{
  font-size: 25rpx;
  color: #A7A7A7;
}
.projectList-item-3{
  color: #CC9933;
}


.projectList-title-name{
  border-bottom: 1px solid #000000;
  height: 48rpx;
}
.projectList-title-name>text{
  display: inline-block;
  width: 135rpx;
  background-color:#169BD5;
  height: 48rpx;
  line-height: 48rpx;
  color: #FEFEFE;
  text-align: center;
  font-size: 25rpx;
  font-weight: 400;
}
.projectList-title-img{
  margin:  0 auto;
  margin-top: 50rpx;
  width: 90%;
  height:595rpx;
}
.projectList-title-img image{
  width: 100%;
  height: 100%;
}

.projectList-advisor-item{
  font-size: 25rpx;
  padding: 10rpx 0;
  border-bottom: 1rpx solid #A0A0A0;
}
.advisor-content-top{
  color: #9D9D9D;
  height: 100%;
}

.col{
  height: 60rpx;
  line-height:60rpx;
}
.projectList-advisor{
  padding-top: 50rpx;
  color: #9D9D9D;
}
.projectList-advisor-title{
  font-size:35rpx !important;
  font-weight: 500;
  padding: 10rpx 0;
  color: #000000;
}
.advisor-content-top>view{
  height: 60rpx;
}
.advisor-name,.advisor-project{
  line-height: 60rpx;
  padding: 15rpx 0;
}

.advisor-name-title{
  font-size: 40rpx;
  color: #000000;
}

.advisor-name image{
  width: 16rpx;
  height: 20rpx;
}

.advisor-item-img image{
  width: 121rpx;
  height: 121rpx;
  border-radius: 50%;
}


.advisor-btn{
  width: 100%;
  height: 73rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  background-color: #169BD5;
}
.advisor-btns{
  color: #FEFEFE;
  text-align: center;
  line-height: 73rpx;
}