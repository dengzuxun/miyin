
page{
  height: 100%;
}

.container{
  background:#F9FBFF;
  height: 100%;
}
.my-bak{
  background-color: #FF928D;
  position: relative;
}
.my-title{
  display: flex;
  margin: 0 30rpx;
  height: 257rpx;
}

.my-name{
  margin-left: 10rpx;
}
.my-name text:nth-of-type(1){
  font-size: 35rpx;
  color: #fff;
  font-weight: 500;
  margin-right: 20rpx;
}
.my-name text:nth-of-type(2){
  font-size: 25rpx;
  color: #949191;
  font-weight: 500;
}

.my-input{
  display: flex;
  height: 20rpx;
  margin-top: 8rpx;
}
.my-input input{
  height: 20rpx !important;
  min-height: 15rpx !important;
  font-size: 20rpx;
  display: inline-block;
  background: #d8b3b3;
  border-radius: 20rpx;
  text-align: center;
  /* margin: 0 10rpx; */
  /* color: #fff; */
}
.my-input image{
  width: 16rpx;
  height: 16rpx;
  margin-left: 5rpx;
}
.my-img image{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}

.my-list-box{
  position: absolute;
  width: 690rpx;
  color: #333333;
  left: 50%;
  border-radius: 20rpx;
}
.my-box{
  height: 136rpx;
  font-size: 35rpx;
  display: flex;
  bottom: 0;
  transform: translate(-50%,50%);
  justify-content: space-around;
  text-align: center;
  align-items:center;
  background-color: #fff;
}

.my-box text{
  font-size: 25rpx;
  color: #7F8389;
}

.my-Order-items{
  margin:0 30rpx;
  margin-top: 90rpx;
}

.my-Order-item{
  background-color: #fff;
  border-radius:20rpx;
}
.my-Order-title{
  display: flex;
  padding: 25rpx;
  padding-top:35rpx ;
  font-size: 28rpx;
  color:#222222;
  justify-content: space-between;
}

.my-Order-title view:nth-of-type(2){
    font-size: 25rpx;
    color: #979797;
}

.my-Order-list{
  display: flex;
  justify-content: space-around;
  text-align: center;
  font-size: 20rpx;
  color: #7F8389;
  padding: 25rpx 0;
}
.my-Order-btn{
  padding: 0 30rpx;
}

.my-Order-img{
  padding-bottom: 12rpx;
}
.my-Order-img image{
  width: 44rpx;
  height: 44rpx;
}

.my-service{
  background-color: #fff;
  margin-top: 30rpx;
  border-radius: 20rpx;
}

.my-service>view:nth-of-type(1){
  padding:25rpx;
 
}
.my-service-list{
  display: flex;
  flex-wrap: wrap;
}
.my-service-list image{
  width: 44rpx;
  height: 44rpx;
}

.my-service-nape{
  width: 25%;
  font-size: 25rpx;
  text-align: center;
  color: #7F8389;
  padding: 25rpx 0;
}
.my-service-img{
  margin-bottom: 20rpx;
}